rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Partner logos
    match /partners/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    
    // Instructor images
    match /instructors/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
  }
}
